books = [
    ("Python Programming", 15),
    ("Data Structures", 8),
    ("Algorithms", 12),
    ("Operating Systems", 5),
    ("Computer Networks", 0),
    ("AI Basics", 12)
]

def average_borrow(books):
    total = sum(borrow for _, borrow in books)
    avg = total / len(books)
    return avg

def highest_lowest(books):
    highest = max(books, key=lambda x: x[1])
    lowest = min(books, key=lambda x: x[1])
    return highest, lowest

def count_zero_borrowers(books):
    count = sum(1 for _, borrow in books if borrow == 0)
    return count

from collections import Counter
def most_frequent_borrow(books):
    counts = [borrow for _, borrow in books]
    freq = Counter(counts)
    most_common_count = freq.most_common(1)[0][0]
    most_frequent_books = [book for book, borrow in books if borrow == most_common_count]
    return most_common_count, most_frequent_books

avg = average_borrow(books)
highest, lowest = highest_lowest(books)
zero_borrowers = count_zero_borrowers(books)
most_freq_count, most_freq_books = most_frequent_borrow(books)

print("Average books borrowed:", avg)
print("Highest borrowed book:", highest)
print("Lowest borrowed book:", lowest)
print("Members with 0 borrowings:", zero_borrowers)
print("Most frequent borrow count:", most_freq_count)
print("Books with that count:", most_freq_books)

print("\nTime and Space Complexities:")
print("average_borrow() - Time: O(n), Space: O(1)")
print("highest_lowest() - Time: O(n), Space: O(1)")
print("count_zero_borrowers() - Time: O(n), Space: O(1)")
print("most_frequent_borrow() - Time: O(n), Space: O(n)")
